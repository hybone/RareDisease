page{
    display: block;  
    min-height: 100%;  
    background-color:#f6f6f6;  
    font-family: "微软雅黑";
    font-size: 30rpx;
    color:#353535;
  }
  .container{
    margin-top: 20rpx;
    padding-bottom: 110rpx;
  }
  
  /* 地址信息 */
  .item{
    width: 100%;
    height: 140rpx;
    background: white;
    position: relative;
    border-bottom: 1rpx solid #e0e0e0;
    margin: 10rpx 10rpx 10rpx 10rpx;
  }
  .info1{
    position: absolute;
    width: 80%;
    left: 30rpx;
    top: 20rpx;
    font-size:26rpx;
    line-height:36rpx;
    display: inline-block;
    white-space: nowrap; 
    overflow: hidden;
    text-overflow:ellipsis;
  }
  .info2{
    position: absolute;
    left: 30rpx;
    top: 80rpx;
    color:#353535;
    font-size:30rpx;
    padding-right: 50px;
  }
  .edit{
    position: absolute;
    right: 60rpx;
    top: 45rpx;
    width: 50rpx;
    height: 50rpx;
  }