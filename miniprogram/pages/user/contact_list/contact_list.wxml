<view>
  <van-cell 
    wx:for="{{chatList}}" 
    wx:key="receiverId"
    data-id="{{item._id}}"
    bind:click="openChat"
  >
    <image slot="icon" src="{{item.avatarUrl}}" class="cell-image" />
    <view slot="title">{{item.name}}</view>
    <!-- <view slot="title" class="message">{{ item.content }}</view> -->
  </van-cell>
</view>
